{% set headingLevel = params.headingLevel if params.headingLevel else 3 %}
<div class="moduk-do-dont{%- if params.classes %} {{ params.classes }}{% endif %}"{%- for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %}>
  <h{{ headingLevel }} class="moduk-do-dont__label">{{ params.title }}</h{{ headingLevel }}>
  <ul class="moduk-do-dont__list {% if params.type == 'tick' %}moduk-do-dont__list--tick{% else %}moduk-do-dont__list--cross{% endif %}">
    {%- for data in params.items %}
    <li>
    {%- if params.type == 'cross' %}
      <span class="moduk-do-dont__cross" aria-hidden="true"></span>
    {%- else %}
      <span class="moduk-do-dont__tick" aria-hidden="true"></span>
    {%- endif %}{% if params.type == 'cross' %}{% if not params.hidePrefix %}Do not {% endif %}{% endif %}{{ data.item | safe}}
    </li>
    {%- endfor %}
  </ul>
</div>